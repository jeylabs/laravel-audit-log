{
  "name": "jeylabs/laravel-audit-log",
  "description": "A very simple audit logger to monitor the users of your website or application",
  "homepage": "https://github.com/jeylabs/laravel-audit-log",
  "keywords": [
    "jeylabs",
    "log",
    "user",
    "activity",
    "audit",
    "laravel"
  ],
  "authors": [
    {
      "name": "jeylabs",
      "email": "ratheep.ceymplon@jeylabs.com.au",
      "homepage": "https://jeylabs.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
   "illuminate/config": "^6.0|^7.0|^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "ext-json": "*",
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Jeylabs\\AuditLog\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Jeylabs\\AuditLog\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "license": "MIT"
}
